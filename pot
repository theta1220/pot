
// ./sumi_src/main.ss

var main = octopus.main.new();
main.entry("hello");


// ./octopus/src/main.ss

class octopus
{
    class main : object
    {
        func entry(text)
        {
            log("input command: {0}", text);
        }
    }
}


// ./octopus/src/system.ss



// ./octopus/src/log.ss

func log(text, args)
{
    system_call("Pot.Sumi.Log", null, text, args);
}


// ./octopus/src/object.ss

class object
{
    func new()
    {
        return @this;
    }
}


